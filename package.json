{
  "name": "@stateform/element",
  "version": "0.1.4",
  "main": "dist/stateform-element.common.js",
  "browser": "dist/stateform-element.umd.min.js",
  "files": [
    "dist"
  ],
  "description": "StateForm implementation of element",
  "repository": "https://github.com/stateform/stateform-element",
  "keywords": [
    "stateform",
    "vue",
    "element-ui",
    "element",
    "form",
    "generator"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build --target lib --name stateform-element src/lib/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "element-ui": "^2.4.9",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
